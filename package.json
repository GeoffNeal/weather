{
  "name": "collinson",
  "packageManager": "yarn@4.10.3",
  "type": "module",
  "engines": {
    "node": ">=20.19",
    "yarn": ">=4.0"
  },
  "scripts": {
    "test": "jest",
    "lint": "eslint --fix",
    "build:server": "webpack --config webpack.config.server.js",
    "build:client": "webpack --config webpack.config.client.js",
    "build": "yarn build:server && yarn build:client",
    "start:server": "node dist/server/server.bundle.js",
    "start:client": "node dist/server/webserver.bundle.js",
    "start": "yarn build && concurrently \"yarn start:server\" \"yarn start:client\"",
    "codegen": "graphql-codegen --config codegen.ts",
    "postinstall": "husky || true"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/plugin-transform-modules-commonjs": "^7.27.1",
    "@babel/plugin-transform-runtime": "^7.28.3",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@graphql-codegen/cli": "^6.0.0",
    "@graphql-codegen/typescript": "^5.0.0",
    "@graphql-codegen/typescript-resolvers": "^5.0.0",
    "@types/babel__core": "^7",
    "@types/babel__plugin-transform-runtime": "^7",
    "@types/babel__preset-env": "^7",
    "@types/express": "^5",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.5.2",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "babel-loader": "^10.0.0",
    "concurrently": "^9.2.1",
    "css-loader": "^7.1.2",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "html-webpack-plugin": "^5.6.4",
    "husky": "^9.1.7",
    "jest": "^30.1.3",
    "lint-staged": "^16.2.0",
    "mini-css-extract-plugin": "^2.9.4",
    "pinst": "^3.0.0",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.4",
    "ts-loader": "^9.5.4",
    "typescript": "^5.9.2",
    "webpack": "^5.101.3",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "@apollo/client": "^4.0.6",
    "@apollo/datasource-rest": "^6.4.1",
    "@apollo/server": "^5.0.0",
    "express": "^5.1.0",
    "graphql": "^16.11.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  }
}
